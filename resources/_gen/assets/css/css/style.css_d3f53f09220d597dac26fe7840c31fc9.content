body{font-family:helevetica,sans-serif;line-height:1.6;font-size:16px;color:#222;margin:0;background-color:#f0f0f0}#main{width:90%;margin:auto;display:flex;justify-content:center;padding-top:5vh}#toc{padding-top:100px;padding-right:60px;min-width:150px}#content{max-width:650px;margin:auto}#top{border-top:10px solid #722;color:#722;font-family:pt mono,monospace}a{color:#c00}#top a,#top a:visited{color:inherit}#site-name{margin:0;padding:15px 45px}h1,h2,h3,th{font-family:pt mono,monospace;line-height:1.1}p{text-align:justify}#toc-fixed{top:0;font-weight:500;position:sticky;left:5vw;padding-top:20px}#toc ul{list-style-type:none;padding:0}#toc ul>li>ul>li>ul{display:none}#toc li{list-style-type:none}p img{vertical-align:middle}pre{padding:10px;color:#fafafa;background-color:#300}td,th{min-width:1%;text-align:left}table{padding:10px 0}hr{border:1px solid #300}